{
  "changeLoadToInit" : 0,
  "autoExpose" : {
    "enabled" : 1,
    "defaultShowOutput" : 1,
    "showOutput" : [],
    "include" : [],
    "exclude" : [
        "printCSS", "etmUpdate", "effectEditorUpdate", 
        "bindScoresUpdate", "openDatabaseUpdate", "Configure",
        "onCampaignLoad", "onFirstInitialization", "effectEditor",
        "effectsLibrary", "effectsTokenManager", "openDatabase", "bindScores"
    ]
  },
    "library.json" : {
     "authors" : ["Witness1"],
     "name" : "Lib:Mod",
     "licenseFile" : "LICENSE.TXT",
     "license" : "WTFPL",
     "shortDescription" : "LibMod is a library for buff and effect handling.",
     "namespace" : "w1.lmod",
     "allowsUriAccess" : true,
     "website" : "https://github.com/StefanoMarina/mt-mods",
     "version" : "1.1.0",
     "readMeFile" : "README.md",
     "description" : "LibMod allows certain d20 operations such as raw stat to modifier (ie. 15=>+1), and management of modifiers, buffs and effects.",
     "gitUrl" : "https://github.com/StefanoMarina/mt-mods.git"
  },
	"macros": {
		"folder" : "library/mtscript/public",
		"map" : "macro_script_map.txt",
    "updatePropertiesFile": 1,
		"exclude": ["onCampaignLoad"],
		"include" : []
	},
	"events.json": {
	  "events" : [
      { "name" : "onFirstInit", "mts" : "onFirstInit" },
      { "name" : "onInit", "mts" : "onInit" }
	  ]
	},
  "zip" : {
    "exclude" : ["CHANGELOG.md"]
  }
}
